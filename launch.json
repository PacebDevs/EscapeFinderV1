{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Chrome contra Ionic",
      "type": "pwa-chrome",
      "request": "launch",
      "url": "http://localhost:8100",
      "webRoot": "${workspaceFolder}",
      "sourceMaps": true
    },
    {
      "name": "Attach a Chrome (puerto 9222)",
      "type": "pwa-chrome",
      "request": "attach",
      "port": 9222,
      "webRoot": "${workspaceFolder}",
      "sourceMaps": true
    }
  ]
}
